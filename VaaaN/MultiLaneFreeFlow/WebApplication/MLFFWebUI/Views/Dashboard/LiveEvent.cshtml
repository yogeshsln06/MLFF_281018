
@{
    ViewBag.Title = "LiveEvent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid" style="overflow-y:scroll; height:75vh;">
    <div class="row">
        <div class="col-md-12">
            <div class="bgc-white bd bdrs-3 p-20 mB-20">
                
                <div class="panel panel-info">
                    <div class="panel-body">
                        <div class="panel-group" id="accordion">
                            <div class="panel panel-default">
                                <h5 class="c-grey-900 heading"><strong>IKE </strong>Information<span></span></h5>
                                <div id="collapse1" class="panel-collapse collapse in" style="display:block!important">
                                    <div class="panel-body table-responsive">
                                        <table id="tableCrossTalk" class="table table-condensed table-striped table-hover my-table-bordered">
                                            <thead>
                                                <tr>
                                                    <th style="text-align:left; width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.Gantry
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        Lajur
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleClass
                                                    </th>
                                                    <th style="width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleRegNo
                                                    </th>
                                                    <th style="width:60px;">
                                                        Date Time
                                                    </th>
                                                    <th style="width:60px;">
                                                        Tag Id
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <h5 class="c-grey-900 heading"><strong>ANPR Front </strong>Information<span></span></h5>
                                <div id="collapse2" class="panel-collapse collapse in" style="display:block!important">
                                    <div class="panel-body table-responsive">
                                        <table id="tableNodeFluxFront" class="table table-condensed table-striped table-hover">
                                            <thead>
                                                <tr>
                                                    <th style="text-align:left; width:60px;">
                                                        Gantry
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        Lajur
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleClass
                                                    </th>
                                                    <th style="width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleRegNo
                                                    </th>
                                                    <th style="width:60px;">
                                                        Date Time
                                                    </th>
                                                    <th style="width:60px;">
                                                        Files
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <h5 class="c-grey-900 heading"><strong>ANPR Rear </strong>Information<span></span></h5>
                                <div id="collapse3" class="panel-collapse collapse in" style="display:block!important">
                                    <div class="panel-body table-responsive">
                                        <table id="tableNodeFluxRear" class="table table-condensed table-striped table-hover">
                                            <thead>
                                                <tr>
                                                    <th style="text-align:left; width:60px;">
                                                        Gantry
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        Lajur
                                                    </th>
                                                    <th style="text-align:left; width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleClass
                                                    </th>
                                                    <th style="width:60px;">
                                                        @VaaaN.MLFF.Libraries.CommonLibrary.Resource.Resourceen.VehicleRegNo
                                                    </th>
                                                    <th style="width:60px;">
                                                        Date Time
                                                    </th>
                                                    <th style="width:60px;">
                                                        Files
                                                    </th>

                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
</div>

<style>
    .well {
        min-height: 20px;
        padding: 5px;
        background-color: #f5f5f5;
        border: 1px solid #e3e3e3;
        border-radius: 4px;
        -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
        box-shadow: inset 0 1px 1px rgba(0, 0, 0, .05);
    }

    .panel-default > .panel-heading {
        color: #fff;
        font-weight: bold;
        background-color: #40b2cd;
        border-color: #40b2cd;
        text-decoration: none;
    }

    a:hover {
        text-decoration: none;
    }

    a:active {
        text-decoration: none;
    }
    /**/
    /* The Modal (background) */

    .modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 1; /* Sit on top */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    }

    /* Modal Content/Box */
    .modal-content {
        background-color: #fefefe;
        margin: 5% auto;
        padding: 20px;
        border: 1px solid #888;
        width: 800px;
        height: 300px;
    }

    /* The Close Button */
    .close {
        color: #aaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: black;
            text-decoration: none;
            cursor: pointer;
        }

    .videoDiv {
        height: 150px;
        width: 200px;
    }

    .panel-body {
        padding: 0px;
    }
</style>

<script src="~/js/liveEvents.js"></script>
<script src="~/Scripts/jquery.freezeheader.js"></script>