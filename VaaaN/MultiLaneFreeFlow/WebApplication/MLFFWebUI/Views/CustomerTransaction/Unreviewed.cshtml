@model VaaaN.MLFF.Libraries.CommonLibrary.CBE.ViewTransactionCBE
@{
    ViewBag.Title = "Unreviewed Transcation";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="bgc-white bd bdrs-3 p-10 mB-10">
                <div class="form-row">
                    <div class="col-md-8">
                        <div class="mT-10">
                            <h4 class="c-grey-900 mB-20">Unreviewed Transcation</h4>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="mT-10">
                            <div class="gap-5 peers" style="float:right;overflow: visible !important;">
                                <div class="peer">
                                    <a class="btn cur-p  btn-outline-primary" href='javascript:void(0);' onclick="reloadData();">
                                        <span class='icon-holder'>
                                            <i class='c-blue-500 ti-reload'></i>
                                        </span>
                                    </a>

                                </div>
                                <div class="peer">
                                    <span class="btn cur-p  btn-outline-primary" data-toggle="dropdown" aria-expanded="true" onclick="openFilter(this)">
                                        <i class='c-black-500 ti-filter'></i>
                                    </span>
                                    <div class="dropdown-menu dropdown-menu-right myfilter" role="menu" id="ddlFilter" style="width:250px; left:-50px!important;">
                                        <div class="mT-5">
                                            <form class="form-horizontal">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.StartDate, new { htmlAttributes = new { @class = "form-control form_datetime", @readonly = true } })
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.EndDate, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.EndDate, new { htmlAttributes = new { @class = "form-control form_datetime", @readonly = true } })
                                                </div>
                                                <div class="form-group">
                                                    @Html.Label("Gantry", htmlAttributes: new { @class = "control-label" })
                                                    @Html.DropDownList("ddlGantry", (IEnumerable<SelectListItem>)ViewBag.Gantry, new { @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    @Html.Label("Vehicle Class", htmlAttributes: new { @class = "control-label" })
                                                    @Html.DropDownList("ddlVehicleClass", (IEnumerable<SelectListItem>)ViewBag.VehicleClass, new { @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    @Html.Label("Transaction Category", htmlAttributes: new { @class = "control-label" })
                                                    @Html.DropDownList("ddlTransactionCategory", (IEnumerable<SelectListItem>)ViewBag.TransactionCategory, new { @class = "form-control" })
                                                </div>

                                                <button type="button" class="btn btn-primary icon-holde" style="float: right;margin-right: 5px;">
                                                    <i class='c-grey-900 ti-search'></i>
                                                </button>
                                            </form>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <table id="tblUnreviewedData" class="table table-striped  my-table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2">
                                No
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2">
                                ID
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id">
                                Date Time
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id">
                                Gantry
                            </th>
                            <th style="text-align: center; " colspan="2">
                                EVI
                            </th>
                            <th style="text-align: center; " colspan="4">
                                ANPR Front
                            </th>
                            <th style="text-align: center; " colspan="4">
                                ANPR Rear
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id" class="no-rt-border">
                                Speed
                                <br />
                                (km/h)
                            </th>
                        </tr>
                        <tr>
                            <th style="text-align: center; " data-field="price">
                                VRN
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                Class
                            </th>
                            <th style="text-align: center; " data-field="price">
                                VRN
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                Class
                            </th>
                            <th style="text-align: center; " data-field="price">
                                Image
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                Video
                            </th>
                            <th style="text-align: center; " data-field="price">
                                VRN
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                Class
                            </th>
                            <th style="text-align: center; " data-field="price">
                                Image
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                Video
                            </th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#AssociatedModal" id="btnAssociatedModalOpen" style="display:none;">Launch demo modal</button>
<div class="modal fade bd-example-modal-lg" id="AssociatedModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Associate Transcation</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="btnpopupClose"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body" style="height:400px; overflow-y:scroll;">
                <div id="partialassociated">

                </div>
            </div>
            <div class="modal-footer">
                <div class="row col-md-12" style="float:left;">
                    <label class="col-md-1 control-label" style="padding-top: 5px;">VRN:</label>
                    <input type="text" id="txtVRN" class="col-md-2 form-control" placeholder="VRN" />
                    <label class="col-md-1 control-label" style="padding-top: 5px;">Class:</label>
                    @Html.DropDownList("ddlAuditedVehicleClass", (IEnumerable<SelectListItem>)ViewBag.VehicleClass, new { @class = "col-md-2 form-control" })
                    <div class="col-md-3"></div>
                    <div class="col-md-2">
                        <button type="button" class="btn cur-p btn-warning " onclick="SaveUnidentified()">Unidentified</button>
                    </div>
                    <div class="col-md-1">
                        <button type="button" class="btn cur-p btn-secondary" onclick="Complete()">Complate</button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/CustomerTranscation.js"></script>
<script>
    $(document).ready(function () {
        $(".form_datetime").datetimepicker({
            format: "mm/dd/yyyy  hh:ii",
            autoclose: true,
            todayBtn: true,
            startDate: "2018-12-06 10:00",
            minuteStep: 30
        })
        BindDateTime();
        BindUnreviewedFirstLoad();
    });
</script>
<link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />

<script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>