@{
    ViewBag.Title = "Violation Transcation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="bgc-white bd bdrs-3 p-20 mB-20">
                <div class="form-row">
                    <div class="col-md-8">
                        <div class="mT-10">
                            <h4 class="c-mygrey-900 mB-20">Violation Transcation</h4>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="mT-10">
                            <div class="gap-5 peers" style="float:right;overflow: visible !important;">
                                <div class="peer">
                                    <a class="btn cur-p btn-light" href='javascript:void(0);' onclick="reloadViolationData();">
                                        <span class='icon-holder'>
                                            <i class='c-blue-500 ti-reload'></i>
                                        </span>
                                    </a>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <table id="tblViolationData" class="table table-striped  my-table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2">
                                <div class="th-inner sortable both">#</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2">
                                <div class="th-inner sortable both">ID</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id">
                                <div class="th-inner sortable both">Date Time</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id">
                                <div class="th-inner sortable both">Gantry</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " colspan="2">
                                <div class="th-inner ">EVI</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " colspan="4">
                                <div class="th-inner ">ANPR Front</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " colspan="4">
                                <div class="th-inner ">ANPR Rear</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; vertical-align: middle; " rowspan="2" data-field="id" class="no-rt-border">
                                <div class="th-inner sortable both">Speed</div>
                                <div class="fht-cell"></div>
                            </th>

                        </tr>
                        <tr>
                            <th style="text-align: center; " data-field="price">
                                <div class="th-inner sortable both">VRN</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                <div class="th-inner sortable both">Class</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="price">
                                <div class="th-inner sortable both">VRN</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                <div class="th-inner sortable both">Class</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="price">
                                <div class="th-inner">Image</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                <div class="th-inner">Video</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="price">
                                <div class="th-inner sortable both">VRN</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                <div class="th-inner sortable both">Class</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="price">
                                <div class="th-inner">Image</div>
                                <div class="fht-cell"></div>
                            </th>
                            <th style="text-align: center; " data-field="operate">
                                <div class="th-inner">Video</div>
                                <div class="fht-cell"></div>
                            </th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        BindViolationFirstLoad();
    });
</script>
<script src="~/js/CustomerTranscation.js"></script>

