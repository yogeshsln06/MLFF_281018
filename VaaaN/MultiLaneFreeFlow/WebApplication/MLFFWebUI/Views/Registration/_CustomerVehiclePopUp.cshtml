    <div class="modal-footer">
        <div class="alert alert-warning" role="alert" id="warning">
        </div>
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="btnpopupClose">Close</button>
        <button type="button" class="btn btn-primary" onclick="SaveData('new')">Save & New</button>
        <button type="button" class="btn btn-primary" onclick="SaveData('close')">Save changes</button>
    </div>
