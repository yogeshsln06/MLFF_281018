@*@model IEnumerable<VaaaN.MLFF.Libraries.CommonLibrary.CBE.UserCBE>*@
@{
    ViewBag.Title = "Users List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="bgc-white bd bdrs-3 p-20 mB-20">
                <div class="form-row">
                    <div class="col-md-8">
                        <div class="mT-10">
                            <h4 class="c-mygrey-900 mB-20">System User</h4>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="mT-10">
                            <div class="gap-5 peers" style="float:right;overflow: visible !important;">
                                <div class="peer">
                                    <button type="button" class="btn cur-p btn-outline-primary" onclick="NewUser();">New</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <table id="tblUserData" class="table table-striped table-bordered table-condensed table-hover" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                           <th id="tblUserDataTR" style="text-align: center; vertical-align: middle;">No</th>
                            <th style="text-align:center; vertical-align: middle;">Login Name</th>
                            <th style="text-align:center; vertical-align: middle;">Name</th>
                            <th style="text-align:center; vertical-align: middle;">Role</th>
                            <th style="text-align:center; vertical-align: middle;" class="no-rt-border"></th>
                         </tr>
                    </thead>
                    <tbody></tbody>
                </table>

            </div>
        </div>
    </div>
</div>
<div class="modal fade bd-example-modal-md" id="customerModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Register Customer</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div id="partialassociated">

            </div>
        </div>
    </div>
</div>
<script src="~/js/SetUp.js"></script>
<script>
    $(document).ready(function () {
        UserDataList = @Html.Raw(Json.Encode(ViewData["UserDataList"]));
        BindUserData(UserDataList);
    });
</script>
